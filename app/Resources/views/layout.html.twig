<!DOCTYPE html>
<html lang="fr" prefix="og: http://ogp.me/ns#">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <title>{% block title %}Micro-nutrition Nutrition Sport et Sant√©{% endblock %}</title>
    <meta name="description" content="{% block description %} {% endblock %}">
    {% block stylesheets %}
        <link href="{{ asset("css/bootstrap.min.css") }}" rel="stylesheet">
        <link href="{{ asset("css/mdb.min.css") }}" rel="stylesheet">
        <link href="{{ asset("css/style.min.css") }}" rel="stylesheet">
    {% endblock %}
    <meta name="category" content="">
    <meta name="publisher" content="">
    <link href="" rel="publisher" />
    {% block open_graph %}
        <meta property="og:url" content="{{ app.request.uri }}" />
        <meta property="og:type" content="website" />
        <meta property="og:title" content="" />
        <meta property="og:image" content="{{ app.request.scheme ~'://' ~ app.request.httpHost }}" />
    {% endblock %}
</head>
<body class="third-color">
{% include':elements:navigation.html.twig' %}
{% block central_layout %}

{% endblock %}
{% include':elements:footer.html.twig' %}
{% block javascripts %}
    <script src="https://use.fontawesome.com/cf32ea05ba.js"></script>
    <script type="text/javascript" src="{{ asset("js/jquery-3.1.1.min.js") }}"></script>
    <script type="text/javascript" src="{{ asset("js/tether.min.js") }}"></script>
    <script type="text/javascript" src="{{ asset("js/bootstrap.min.js") }}"></script>
    <script type="text/javascript" src="{{ asset("js/mdb.min.js") }}"></script>
    <script>
        $(".button-collapse").sideNav();
    </script>
{% endblock %}
</body>
</html>